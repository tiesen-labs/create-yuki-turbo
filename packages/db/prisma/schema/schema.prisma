generator client {
  provider        = "prisma-client"
  previewFeatures = ["prismaSchemaFolder", "driverAdapters"]
  binaryTargets   = ["native", "rhel-openssl-3.0.x"]

  output              = "../generated"
  moduleFormat        = "esm"
  importFileExtension = ""
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
